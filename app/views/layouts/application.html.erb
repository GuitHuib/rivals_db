<!DOCTYPE html>
<html>

  <%= render 'layouts/head' %>

  <body>
    <%= render 'layouts/header' %>
    <% flash.each do |type, msg| %>
      <div>
        <div class="alert alert-<%= type %>"><%= msg %></div>
      </div>
    <% end %>
    <%= yield %>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>
