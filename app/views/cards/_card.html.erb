<div class="row card-row">
  <div class="col-md-2">
    <%= link_to card.name, card, remote:true %>
  </div>
  <div class="col-md-2">
    <%= card.faction %>
  </div>
  <div class="col-md-2">
    <%= card.card_type %>
  </div>
  <div class="col-md-2">
    <%= card.blood %>
  </div>
</div>
<div id="card<%= card.id %>"></div>
