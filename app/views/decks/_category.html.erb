<div id=<%= type %>>
  <div class="row">
    <h4><%= title %> (<span id="<%= type %>-count"><%= cards.length %></span>)</h4>
  </div>
  <div class="card-container row">
    <% cards.uniq.each do |card| %>
    <!-- render each card with count -->
      <%= render 'decks/deckCard',
        card: card,
        card_count: cards.where(name: card.name).length %>
    <% end %>
  </div>
</div>
